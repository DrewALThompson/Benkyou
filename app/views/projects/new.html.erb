<%= render 'errors' %>
<div class='new-form'>
  <%= form_for(@project) do |f| %>
    <%= f.label :title %><br>
    <%= f.text_field :title %><br>
    <%= f.label :content %><br>
    <%= f.text_area :content %><br>
    <%= f.hidden_field :owner_id, value: current_user.id %>
    <%= f.submit "Create Project" %><br>
  <% end %>
</div>