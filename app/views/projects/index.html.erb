<% @projects.each do |project| %>
<br>
<div class='projects'>
  <div class='titles'>
    <div class='title'>
      <%= link_to "#{project.title}", project_path(project) %>
    </div>
    <div class='trns-title'>
      <%= project.translation_title%>
    </div>
  </div>
  <div class='contents'>
    <%= project.content %>
  </div>
  <div class='trns'>
    <%= project.translation %>
  </div>
  <div class='owner'>
    By <%= project.owner.name %>
  </div>
</div>  
<% end %>
