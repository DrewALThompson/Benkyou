<!DOCTYPE html>
<html>
  <head>
    <title>Benkyou</title>
    <%= csrf_meta_tags %>
    <%= csp_meta_tag %>

    <%= stylesheet_link_tag 'application', media: 'all', 'data-turbolinks-track': 'reload' %>
    <%= javascript_pack_tag 'application', 'data-turbolinks-track': 'reload' %>
  </head>

  <body>
  
  <%= render 'layouts/header'%>
  <div class='flash'>
  <% flash.each do |type, msg| %>
    <div class='flash-msg'>
      <h2><%= msg %></h2>
    </div>
  <% end %>
  </div>
  
    <%= yield %>
  </body>
</html>
